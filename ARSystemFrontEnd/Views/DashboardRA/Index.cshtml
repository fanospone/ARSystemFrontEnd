
@{
    ViewBag.Title = "Monitoring KPI RA";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/dashboard/css/iconFamily.css")" rel="stylesheet" type="text/css">
<link href="@Url.Content("~/Content/dashboard/css/style.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/dashboard/css/themes/all-themes.css")" rel="stylesheet" type="text/css" />

<style>
    .fullscreen {
        z-index: 9999;
        width: 100%;
        height: 100%;
        position: fixed;
        top: 0;
        left: 0;
    }

    hr {
        height: 1px;
        color: darkgrey;
        background-color: darkgrey;
        border: none;
    }

    .page-content {
        background-color: #e9e9e9;
        !important;
    }

    #chartdivlt {
        width: 100%;
        height: 500px;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivod {
        width: 100%;
        height: 500px;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivrti {
        width: 100%;
        height: 400px;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }
</style>


<input id="hdUserRole" value="@ViewBag.UserGroup" style="display:none" />
<div class="row">
    <div class="col-md-12">
        <div class="block-header col-md-6" style="margin-left:-15px;">
            <button type="button" class="btn btn-info waves-effect" style="float:left; margin-right:10px" value="TSEL" id="btnGroupTsel" ><b>TSEL</b></button>
            <button type="button" class="btn btn-info waves-effect" style="float:left; margin-right:10px" value="NON TSEL" id="btnGroupNonTsel" disabled><b>Non-TSEL</b></button>
            <button type="button" class="btn btn-info waves-effect" style="float:left;" value="NEW PRODUCT" id="btnGroupNew"><b>NEW Product</b></button>
        </div>
        @*<div class="col-md-3">
                <div class="input-group input-group">
                    <span class="input-group-addon" style="padding-left:10px">
                        <i class="material-icons">event_available</i>
                    </span>
                    <div class="form-line">
                        <select id="sslYear" style="background-color: #e9e9e9" class="form-control show-tick"></select>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="input-group input-group">
                    <span class="input-group-addon" style="padding-left:10px">
                        <i class="material-icons">event_available</i>
                    </span>
                    <div class="form-line">
                        <select id="sslMonth" style="background-color: #e9e9e9" class="form-control show-tick"></select>
                    </div>
                </div>
            </div>*@


        <!-- Widgets -->
    </div>
</div>
<div class="row">
    <div class="row clearfix">
        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="info-box bg-purple hover-expand-effect">
                <div class="icon">
                    <i class="material-icons">event_note</i>
                </div>
                <div class="content">
                    <div class="text">Lead Time Status</div>
                    <div id="ltstatus" class="number" style="display:inline-block;"></div><span>&nbsp</span><span id="percentagelt" style="font-size:14px"></span>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="info-box bg-teal hover-expand-effect">
                <div class="icon">
                    <i class="material-icons">do_not_disturb_on</i>
                </div>
                <div class="content">
                    <div class="text">Overdue Status</div>
                    <div id="odstatus" class="number" style="display:inline-block;"></div><span>&nbsp</span><span id="percentageod" style="font-size:14px"></span>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div class="info-box bg-light-green hover-expand-effect">
                <div class="icon">
                    <i class="material-icons">playlist_add_check</i>
                </div>
                <div class="content">
                    <div class="text">RTI Status</div>
                    <div id="rtistatus" class="number" style="display:inline-block;"></div><span>&nbsp</span><span id="percentagerti" style="font-size:14px"></span>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <div id="btnexport" class="info-box bg-orange hover-expand-effect">
                <div class="icon">
                    <i class="material-icons">assignment</i>
                </div>
                <div class="content">
                    <div class="text">Nearly Overdue Status</div>
                    <div id="nodstatus" class="number" style="display:inline-block;"></div><span>&nbsp</span>
                </div>
            </div>
        </div>
    </div>
    <!-- #END# Widgets -->
    <!-- CPU Usage -->
    <div class="row clearfix">
        <div class="col-md-7" id="cardchartdivlt">
            <div class="card">
                <div class="header bg-purple">

                    <i class="icon material-icons" style="font-size:40px;">equalizer</i>
                    <h2 style="display:inline-block">
                        &nbsp&nbspLead Time RTI<small><span>&nbsp&nbsp</span><span class="yearbilion"></span></small>
                    </h2>
                    <ul class="header-dropdown m-r--5">
                        <li>
                            <a href="javascript:reload('chartdivlt');" data-toggle="cardloading" data-loading-effect="timer" data-loading-color="lightBlue">
                                <i class="material-icons">loop</i>
                            </a>
                        </li>
                        <li class="dropdown">
                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <i class="material-icons">more_vert</i>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a href="javascript:remove('cardchartdivlt');">Remove</a></li>
                                <li><a href="javascript:fullscreen('cardchartdivlt');">Fullscreen</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="body">
                    <div id="chartdivlt" class="chart"> </div>
                </div>
            </div>
        </div>
        <div class="col-md-5" id="cardchartdivod">

            <div class="card">
                <div class="header bg-teal">
                    <i class="icon material-icons" style="font-size:40px;">hourglass_empty</i>
                    <h2 style="display:inline-block">
                        &nbsp&nbspOverdue RTI<small><span>&nbsp&nbsp</span><span class="yearbilion2"></span></small>
                    </h2>
                    <ul class="header-dropdown m-r--5">
                        <li>
                            <a href="javascript:reload('chartdivod');" data-toggle="cardloading" data-loading-effect="timer" data-loading-color="lightBlue">
                                <i class="material-icons">loop</i>
                            </a>
                        </li>
                        <li class="dropdown">
                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <i class="material-icons">more_vert</i>
                            </a>
                            <ul class="dropdown-menu pull-right tools">
                                <li><a href="javascript:remove('cardchartdivod');" class="collapse">Remove</a></li>
                                <li><a href="javascript:fullscreen('cardchartdivod');" class="collapse">Fullscreen</a></li>
                            </ul>
                        </li>
                    </ul>

                </div>
                <div class="body">

                    <div id="chartdivod" class="chart"> </div>

                </div>
            </div>
        </div>
    </div>
    <!-- #END# CPU Usage -->

    <div class="row clearfix">
        <div class="col-md-12" id="cardchartdivrti">
            <div class="card">
                <div class="header bg-indigo">

                    <i class="icon material-icons" style="font-size:40px;">playlist_add_check</i>
                    <h2 style="display:inline-block">
                        &nbsp&nbspFulfilment RTI<small><span>&nbsp&nbsp</span><span class="yearbilion3"></span></small>
                    </h2>
                    <ul class="header-dropdown m-r--5">
                        <li>
                            <a href="javascript:reload('chartdivrti');" data-toggle="cardloading" data-loading-effect="timer" data-loading-color="lightBlue">
                                <i class="material-icons">loop</i>
                            </a>
                        </li>
                        <li class="dropdown">
                            <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                <i class="material-icons">more_vert</i>
                            </a>
                            <ul class="dropdown-menu pull-right">
                                <li><a href="javascript:remove('cardchartdivrti');">Remove</a></li>
                                <li><a href="javascript:fullscreen('cardchartdivrti');">Fullscreen</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <div class="body">
                    <div id="chartdivrti" class="chart"> </div>
                </div>
            </div>
        </div>
    </div>

</div>
</div>

<!-- Resources -->

<script src="@Url.Content("~/Content/dashboard/js/amchart.4.5.9_core.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/dashboard/js/amchart4.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/dashboard/js/animated.js")" type="text/javascript"></script>


<script src="@Url.Content("~/scripts/DashboardRA/MonitoringAchievementRA.js")" type="text/javascript"></script>


