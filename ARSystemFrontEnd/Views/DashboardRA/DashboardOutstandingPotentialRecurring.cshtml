
@{
    ViewBag.Title = "Dashboard Potential Recurring";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


 
    <section id="pnlSummary">
        <div class="row panelFilter">
            <div class="col-md-12">
                <div class="portlet box blue">
                    <div class="portlet-title">
                        <div class="caption font-white">
                            <i class="fa fa-filter"></i>
                            <span class="caption-subject">FILTER</span>
                        </div>
                        <div class="tools">
                            <a href="" class="collapse"> </a>
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <form id="formSearch" class="form-horizontal" role="form">
                            <div class="form-body">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">STIP Date</label>
                                            <div class="col-md-9">
                                                <input type="text" id="tbSearchSTIPDate" size="16" class="form-control datepicker" placeholder="STIP Date">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">RFI Date</label>
                                            <div class="col-md-9">
                                                <input type="text" id="tbSearchRFIDate" size="16" class="form-control datepicker" placeholder="RFI Date">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Section</label>
                                            <div class="col-md-9">
                                                <select id="slSearchSection" class="form-control select2"></select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">SOW</label>
                                            <div class="col-md-9">
                                                <select id="slSearchSOW" class="form-control select2"></select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Product Name</label>
                                            <div class="col-md-9">
                                                <select id="slSearchProduct" class="form-control select2" required></select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">STIP Category</label>
                                            <div class="col-md-9">
                                                <select id="slSearchSTIPCategory" class="form-control select2"></select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Regional</label>
                                            <div class="col-md-9">
                                                <select id="slSearchRegional" class="form-control select2"></select>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-md-3 control-label">Company</label>
                                            <div class="col-md-9">
                                                <select id="slSearchCompany" class="form-control select2"></select>
                                            </div>
                                        </div>

                                    </div>

                                    <br />
                                    <br />
                                    <br />
                                    <br />
                                    <br />
                                    <div class="col-md-1">
                                        <h4><b>Group By</b></h4>
                                    </div>
                                    <div class="col-md-12">

                                    </div>
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <div class="col-md-12">
                                                @*<div class="mt-radio-inline">*@
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchSection" name="rdStatus" value="GroupBySection" checked>Section
                                                    <span></span>
                                                </label>
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchSOW" name="rdStatus" value="GroupBySOW">SOW
                                                    <span></span>
                                                </label>
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchProductName" name="rdStatus" value="GroupByProduct">Product Name
                                                    <span></span>
                                                </label>
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchSTIPCategory" name="rdStatus" value="GroupBySTIPCategory">STIP Category
                                                    <span></span>
                                                </label>
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchRegional" name="rdStatus" value="GroupByRegional">Regional
                                                    <span></span>
                                                </label>
                                                <label class="mt-radio col-md-2">
                                                    <input type="radio" id="rbSearchCompany" name="rdStatus" value="GroupByOperator">Operator
                                                    <span></span>
                                                </label>
                                                @*</div>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-actions text-center">
                                <button type="button" id="btSearch" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                    <i class="fa fa-search"></i> Search
                                </button>
                                <button type="button" id="btReset" class="btn blue-oleo">
                                    <i class="fa fa-refresh"></i> Reset
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="row panelSearchResult portlet-Main">
            <div class="col-md-1">
                <h5><b>Group By</b></h5>
            </div>
            <div class="col-md-12">

            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <div class="col-md-12">
                       
                        <label class="mt-radio col-md-2">
                            <input type="radio" id="rbSearchCount" name="rdStatusMonth" value="" checked>Count
                            <span></span>
                        </label>
                        <label class="mt-radio col-md-2">
                            <input type="radio" id="rbSearchAmount" name="rdStatusMonth" value="Amount">Amount
                            <span></span>
                        </label>
                        
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblSummary">
                    <thead id="tblHeadSummary">

                    </thead>
                    <tfoot>
                        <tr>
                            <td>Grand Total</td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                    </tfoot>
                </table>
            </div>

        </div>
        <div class="row panelSearchResultMonth portlet-Main">

            <div class="col-md-12">
                <table class="table table-striped table-bordered table-hover" id="tblSummaryMonth" style="width:100%">
                    <thead id="tblHeadSummaryMonth">

                    </thead>
                    <tfoot>
                        <tr>
                            <td>Grand Total</td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td></td>
                        </tr>
                    </tfoot>
                </table>
            </div>
            <div class="col-md-2">
                <button type="button" id="btBackFromMonth" class="btn blue-oleo">
                    <i class="fa fa-arrow-left"></i> Back
                </button>
            </div>
        </div>
    </section>

    <section id="pnlDetail" style="display:none;">
        <div class="row panelSearchDetail ">
            <div class="col-md-1">
                <h5><b>Add Filter</b></h5>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <div class="col-xs-10">
                        <div class="form-inline">
                            <div class="form-group">
                                <input type="text" class="form-control" id="slSearchDetailSonum" placeholder="Search by So Number" />
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="slSearchDetailSiteID" placeholder="Search by Site ID" />
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="slSearchDetailSiteName" placeholder="Search by Site Name" />
                            </div>
                            <div class="form-group">
                                <div class="form-actions text-center">
                                    <button type="button" id="btSearchDetail" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                        <i class="fa fa-search"></i>
                                    </button>
                                    <button type="button" id="btResetDetail" class="btn blue-oleo">
                                        <i class="fa fa-refresh"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12">
                <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblDetail">
                    <thead>
                        <tr>
                            <th class="datatable-col-25">No</th>
                            <th class="datatable-col-100">SO Number</th>
                            <th class="datatable-col-100">Site ID</th>
                            <th class="datatable-col-250">Site Name</th>
                            <th class="datatable-col-200">Customer Site ID</th>
                            <th class="datatable-col-250">Customer Site Name</th>
                            <th class="datatable-col-150">Customer ID</th>
                            <th class="datatable-col-150">Region Name</th>
                            <th class="datatable-col-150">Province Name</th>
                            <th class="datatable-col-150">Residence Name</th>
                            <th class="datatable-col-150">RFI Date</th>
                            <th class="datatable-col-150">First BAPS Done</th>
                            <th class="datatable-col-150">STIP Category</th>
                            <th class="datatable-col-150">PO Number</th>
                            <th class="datatable-col-150">MLA Number</th>
                            <th class="datatable-col-150">Start BAPS Date</th>
                            <th class="datatable-col-150">End BAPS Date</th>
                            <th class="datatable-col-130">Term</th>
                            <th class="datatable-col-130">BAPS Type</th>
                            <th class="datatable-col-130">Customer Invoice</th>
                            <th class="datatable-col-130">Company Invoice</th>
                            <th class="datatable-col-130">Company </th>
                            <th class="datatable-col-130">STIP Siro</th>
                            <th class="datatable-col-130">Currency</th>
                            <th class="datatable-col-150">Start Invoice Date</th>
                            <th class="datatable-col-150">End Invoice Date</th>
                            <th class="datatable-col-150">Base Lease Price</th>
                            <th class="datatable-col-150">Service Price</th>
                            <th class="datatable-col-150">Deduction Amount</th>
                            <th class="datatable-col-150">Total Amount</th>

                        </tr>
                    </thead>
                </table>
            </div>
            <div class="col-md-2">
                <button type="button" id="btBack" class="btn blue-oleo">
                    <i class="fa fa-arrow-left"></i> Back
                </button>
            </div>
        </div>
    </section>
 
    

    <a id="dlink" style="display:none;"></a>
    <script src="@Url.Content("~/scripts/DashboardRA/DashboardPotentialRecurring.js")" type="text/javascript"></script>
