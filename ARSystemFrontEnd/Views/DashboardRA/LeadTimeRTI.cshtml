
@{
    ViewBag.Title = "Lead Time RTI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/dashboard/css/iconFamily.css")" rel="stylesheet" type="text/css">
<link href="@Url.Content("~/Content/dashboard/css/style.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/dashboard/css/themes/all-themes.css")" rel="stylesheet" type="text/css" />

<style>
    .fullscreen{
        z-index: 9999; 
        width: 100%; 
        height: 100%; 
        position: fixed; 
        top: 0; 
        left: 0; 
     }
    hr{ 
      height: 1px;
      color: darkgrey;
      background-color: darkgrey;
      border: none;
    }
    .page-content {
        background-color: #e9e9e9;
        !important;
    }

    #chartdiv {
        width: 100%;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivaverage {
        width: 100%;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivsummary {
        width: 100%;
        height: 500px;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivnearlyoverdue {
        width: 100%;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivnearlyoverdueoperators {
        width: 100%;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 14px;
        font-weight: bold;
        opacity: 0.8;
    }

    #chartdivoverdueoperators {
        width: 100%;
        color: rgba(0, 0, 0, 0.5);
        font-family: Calibri;
        font-size: 12px;
        font-weight: bold;
        opacity: 0.8;
    }
</style>



<input id="hdUserRole" value="@ViewBag.UserGroup" style="display:none" />
<div class="row">
    <div class="col-md-12">
        <div class="block-header col-md-3" style="margin-left:-15px;">
            <h2>DASHBOARD</h2>
        </div>
        <div class="col-md-3">&nbsp</div>
        <div class="col-md-3">&nbsp</div>
        <div class="col-md-3">
            <div class="input-group input-group">
                <span class="input-group-addon" style="padding-left:10px">
                    <i class="material-icons">event_available</i>
                </span>
                <div class="form-line">
                    <select id="sslYear" style="background-color: #e9e9e9" class="form-control show-tick"></select>
                </div>
            </div>
            @*"TSEL", "NON TSEL", "NEW PRODUCT"*@
        </div>


        <!-- Widgets -->
        <div class="row clearfix">
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="info-box bg-pink hover-expand-effect">
                    <div class="icon">
                        <i class="material-icons">cast</i>
                    </div>
                    <div class="content">
                        <div class="text">Total Sites TSEL</div>
                        <div id="numbertsel" class="number"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="info-box bg-cyan hover-expand-effect">
                    <div class="icon">
                        <i class="material-icons">cast_connected</i>
                    </div>
                    <div class="content">
                        <div class="text">Total Sites Non-TSEL</div>
                        <div id="numbernontsel" class="number"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div class="info-box bg-light-green hover-expand-effect">
                    <div class="icon">
                        <i class="material-icons">devices_other</i>
                    </div>
                    <div class="content">
                        <div class="text">Total Sites NEW</div>
                        <div id="numbernew" class="number"></div>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
                <div id="btnExport" class="info-box bg-orange hover-expand-effect">
                    <div class="icon">
                        <i class="material-icons">assignment</i>
                    </div>
                    <div class="content">
                        <div class="text">Download ALL</div>
                        <div class="number"><i class="material-icons">get_app</i></div>
                    </div>
                </div>
            </div>
        </div>
        <!-- #END# Widgets -->
        <!-- CPU Usage -->
        <div class="row clearfix">
            <div class="col-md-12" id="cardchartdiv">
                <div class="card">
                    <div class="header bg-purple">

                        <i class="icon material-icons" style="font-size:40px;">equalizer</i>
                        <h2 style="display:inline-block">
                            &nbsp&nbspLead Time RTI by Group<small><span>&nbsp&nbsp</span><span class="yearplusinfo"></span></small>
                        </h2>
                        <ul class="header-dropdown m-r--5">
                            <li>
                                <a href="javascript:reload('chartdiv');" data-toggle="cardloading" data-loading-effect="timer" data-loading-color="lightBlue">
                                    <i class="material-icons">loop</i>
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">more_vert</i>
                                </a>
                                <ul class="dropdown-menu pull-right">
                                    <li><a href="javascript:remove('cardchartdiv');">Remove</a></li>
                                    <li><a href="javascript:fullscreen('cardchartdiv');">Fullscreen</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <div class="body">
                        <button type="button" class="btn btn-info waves-effect" style="float:right;" value="NEW PRODUCT" id="btnGroupNew2"><b>NEW Product</b></button>
                        <button type="button" class="btn btn-info waves-effect" style="float:right; margin-right:10px" value="NON TSEL" id="btnGroupNonTsel2" disabled><b>Non-TSEL</b></button>
                        <button type="button" class="btn btn-info waves-effect" style="float:right; margin-right:10px" value="TSEL" id="btnGroupTsel2"><b>TSEL</b></button>
                        <div id="chartdiv" class="chart" style="height: 400px;"> </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- #END# CPU Usage -->

        <div class="row clearfix">
            <div class="col-md-12" id="cardchartdivaverage">

                <div class="card">
                    <div class="header bg-teal">
                        <i class="icon material-icons" style="font-size:40px;">trending_up</i>
                        <h2 style="display:inline-block">
                            &nbsp&nbspLead Time Average<small><span>&nbsp&nbsp</span><span class="yearbilion"></span></small>
                        </h2>
                        <ul class="header-dropdown m-r--5">
                            <li>
                                <a href="javascript:reload('chartdivaverage');" data-toggle="cardloading" data-loading-effect="timer" data-loading-color="lightBlue">
                                    <i class="material-icons">loop</i>
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                    <i class="material-icons">more_vert</i>
                                </a>
                                <ul class="dropdown-menu pull-right tools">
                                    <li><a href="javascript:remove('cardchartdivaverage');" class="collapse">Remove</a></li>
                                    <li><a href="javascript:fullscreen('cardchartdivaverage');" class="collapse">Fullscreen</a></li>
                                </ul>
                            </li>
                        </ul>
                        
                    </div>
                    <div class="body">
                        <button type="button" class="btn btn-info waves-effect" style="float:right;" value="NEW PRODUCT" id="btnGroupNew"><b>NEW Product</b></button>
                        <button type="button" class="btn btn-info waves-effect" style="float:right; margin-right:10px" value="NON TSEL" id="btnGroupNonTsel" disabled><b>Non-TSEL</b></button>
                        <button type="button" class="btn btn-info waves-effect" style="float:right; margin-right:10px" value="TSEL" id="btnGroupTsel"><b>TSEL</b></button>
                        
                        <div id="chartdivaverage" class="chart"> </div>
                        
                    </div>
                </div>
            </div>

        </div>

    </div>
</div>
<hr/>
<div class="block-header col-md-3" style="margin-left:-15px;">
    <h2>Lead Time Status</h2>
</div>
<div class="col-md-3">&nbsp</div>
<div class="col-md-3">&nbsp</div>
<div class="col-md-3">
    <div class="input-group input-group" style="padding-left:10px">
        <span class="input-group-addon">
            <i class="material-icons">face</i>
        </span>
        <div class="form-line">
            <select id="slSearchCustomerID" style="background-color: #e9e9e9" class="form-control show-tick"></select>
        </div>
    </div>
</div>

<div class="row clearfix" id="leadTimeByStatus">
</div>

