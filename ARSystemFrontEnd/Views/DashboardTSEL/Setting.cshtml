
@{
    ViewBag.Title = "Setting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .height59 {
        height: 59px;
    }

    .panelSearchResult .table-scrollable {
        border: none !important;
    }

    /* End of CSS for fixing layouts in JQuery Datatables with Freeze Columns */
    table tbody tr td {
        font-size: 12px !important;
    }

    th {
        font-size: 12px !important;
        text-align: center;
    }

    .uploadTargetErrorLog {
        max-height: 520px;
        overflow: auto;
    }

    .portlet.light .dataTables_wrapper .dt-buttons {
        position: absolute;
        right: 20px;
        margin-top: 0;
    }
</style>

<section id="pnlSummary">
    <input type="text" id="hdUserToken" value="@ViewBag.UserToken" style="display:none;" />
