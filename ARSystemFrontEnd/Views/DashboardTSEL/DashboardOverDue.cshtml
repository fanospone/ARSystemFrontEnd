
@{
    ViewBag.Title = "Dashboard Overdue";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="pnlSummary">
    <div class="row panelFilter">
        <div class="col-md-12">
            <div class="portlet box blue">
                <div class="portlet-title">
                    <div class="caption font-white">
                        <i class="fa fa-filter"></i>
                        <span class="caption-subject">FILTER</span>
                    </div>
                    <div class="tools">
                        <a href="" class="collapse"> </a>
                    </div>
                </div>
                <div class="portlet-body form">
                    <form id="formSearch" class="form-horizontal" role="form">
                        <div class="form-body">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Year</label>
                                        <div class="col-md-5">
                                            <select id="slSearchYear" class="form-control select2"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6" style="display:none">

                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Month</label>
                                        <div class="col-md-6">
                                            <select id="slSearchMonth" class="form-control select2"></select>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="form-actions text-center">
                            <button type="button" id="btSearch" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                <i class="fa fa-search"></i> Search
                            </button>
                            <button type="button" id="btReset" class="btn blue-oleo">
                                <i class="fa fa-refresh"></i> Reset
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div class="row panelSearchChart">

        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
            <a class="dashboard-stat dashboard-stat-v2 red" href="#" id="divTotal">
                <div class="visual">
                    <i class="fa fa-comments"></i>
                </div>
                <div class="details">
                    <div class="number">
                        <span data-counter="counterup" data-value="66.74" id="PercentSummary">66.74</span> %
                    </div>
                    <div class="desc"><b> Total Overdue </b>  </div>
                    <div class="desc" id="divCountSiteTotal"><b> Tenant </b>  </div>
                </div>
            </a>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
            <a class="dashboard-stat dashboard-stat-v2 blue" href="#" id="divTower">
                <div class="visual">
                    <i class="fa fa-bar-chart-o"></i>
                </div>
                <div class="details">
                    <div class="number">
                        <span data-counter="counterup" data-value="12.54" id="PercentTower">12.54</span> %
                    </div>
                    <div class="desc"><b>Overdue Tower </b>  </div>
                    <div class="desc" id="divCountSiteTower"><b> Tenant </b>  </div>
                </div>
            </a>
        </div>
        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-12">
            <a class="dashboard-stat dashboard-stat-v2 green" href="#" id="divNonTower">
                <div class="visual">
                    <i class="fa fa-shopping-cart"></i>
                </div>
                <div class="details">
                    <div class="number">
                        <span data-counter="counterup" data-value="92.54" id="PercentNonTower">92.54</span> %
                    </div>
                    <div class="desc"><b>Overdue Non Tower</b> </div>
                    <div class="desc" id="divCountSiteNonTower"><b> Tenant </b>  </div>
                </div>
            </a>
        </div>
        <div class="col-md-6">
            <div class="portlet light portlet-fit bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green bold uppercase">Ontime vs Overdue RTI Tower Section</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="vsTower"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="portlet light portlet-fit bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green bold uppercase">Ontime vs Overdue RTI Non Tower Section</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="vsNonTower"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="portlet light portlet-fit bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green bold uppercase">Overdue Tower</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="Tower"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6">
            <div class="portlet light portlet-fit bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green bold uppercase">Overdue Non Tower</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="NonTower"></div>
                </div>
            </div>
        </div>
        @*<div class="col-md-12">
            <div class="portlet light portlet-fit bordered">
                <div class="portlet-title">
                    <div class="caption">
                        <i class="icon-layers font-green"></i>
                        <span class="caption-subject font-green bold uppercase">Achievement</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <div id="Achievement" style="height:500px;"></div>
                </div>
            </div>
        </div>*@
    </div>
</section>

<section id="pnlDetail" style="display:none;">
    <div class="row panelSearchResult ">
        <div class="col-md-12">
            <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblRaw">
                <thead>
                    <tr>
                        <th class="datatable-col-25">No</th>
                        <th class="datatable-col-100">SO Number</th>
                        <th class="datatable-col-100">Site ID</th>
                        <th class="datatable-col-250">Site Name</th>
                        <th class="datatable-col-100">Customer Site ID</th>
                        <th class="datatable-col-250">Customer Site Name</th>
                        <th class="datatable-col-100">Customer ID</th>
                        <th class="datatable-col-100">Region Name</th>
                        <th class="datatable-col-100">Province Name</th>
                        <th class="datatable-col-100">Residence Name</th>
                        <th class="datatable-col-100">PO Number</th>
                        <th class="datatable-col-150">MLA Number</th>
                        <th class="datatable-col-100">Start Lease Date</th>
                        <th class="datatable-col-100">End Lease Date</th>
                        <th class="datatable-col-100">Term</th>
                        <th class="datatable-col-100">BAPS Type</th>
                        <th class="datatable-col-100">Power Type</th>
                        <th class="datatable-col-100">Customer Invoice</th>
                        <th class="datatable-col-100">Company Invoice</th>
                        <th class="datatable-col-100">Company Asset</th>
                        <th class="datatable-col-100">STIP Siro</th>
                        <th class="datatable-col-100">Currency</th>
                        <th class="datatable-col-100">Invoice Start Date</th>
                        <th class="datatable-col-100">Invoice End Date</th>
                        <th class="datatable-col-100">Base Lease</th>
                        <th class="datatable-col-100">Service Price</th>
                        <th class="datatable-col-100">Deduction Amount</th>
                        <th class="datatable-col-100">Total Amount</th>
                        <th class="datatable-col-100">RTI Done</th>
                        <th class="datatable-col-100">BAPS Confirm Date</th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="col-md-2">
            <button type="button" id="btBack" class="btn blue-oleo">
                <i class="fa fa-arrow-left"></i> Back
            </button>
        </div>
    </div>
</section>

<script src="@Url.Content("~/scripts/DashboardTSEL/DashboardOverDue.js")" type="text/javascript"></script>
