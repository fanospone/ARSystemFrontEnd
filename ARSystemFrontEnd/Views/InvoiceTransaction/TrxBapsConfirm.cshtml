                                        <div class="col-md-9">
                                            <select id="slSearchBapsType" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group section-filter-status-dismantle">
                                        <label class="col-md-3 control-label">Status</label>
                                        <div class="col-md-9">
                                            <select id="slStatusDismantle" class="form-control select2">
                                                <option></option>
                                                <option value="NOTDISMANTLE">Not Dismantle</option>
                                                <option value="DISMANTLE">Dismantle</option>
                                            </select>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="form-actions text-center">
                            <button type="button" id="btSearch" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                <i class="fa fa-search"></i> Search
                            </button>
                            <button type="button" id="btReset" class="btn blue-oleo">
                                <i class="fa fa-refresh"></i> Reset
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

