
@{
    ViewBag.Title = "User Confirmation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section id="pnlSummary">
    @*Filter*@
    <div class="row">
        <div class="col-md-12">
            <div class="portlet box blue">
                <div class="portlet-title">
                    <div class="caption font-white">
                        <i class="fa fa-filter"></i>
                        <span class="caption-subject">Filter</span>
                    </div>
                    <div class="tools">
                        <a href="" class="collapse"> </a>
                    </div>
                </div>
                <div class="portlet-body form">
                    <form id="formSearch" class="form-horizontal" role="form">
                        <div class="form-body">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Periode</label>
                                        <div class="col-md-9">
                                            <div class="col-md-5">
                                                <input type="text" id="tbSearchPeriode" size="16" class="form-control datepicker" placeholder="Input Month Period">
                                            </div>
                                            @*<div class="col-md-1">
                                                    To
                                                </div>*@
                                            <div class="col-md-3">
                                                <select id="slSearchPeriodeWeek" class="form-control select2" placeholder="Week"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Customer</label>
                                        <div class="col-md-9">
                                            <select id="slSearchCustomer" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Company</label>
                                        <div class="col-md-9">
                                            <select id="slSearchCompany" class="form-control select2"></select>
                                        </div>
                                    </div>

                                </div>

                                <div class="col-md-6">
                                    <div class="form-group" style="display:none">
                                        <label class="col-md-3 control-label">Department</label>
                                        <div class="col-md-9">
                                            <select id="slSearchDept" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Status</label>
                                        <div class="col-md-9">
                                            <select id="slSearchStatus" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">SO Number</label>
                                        <div class="col-md-9">
                                            <input type="text" id="tbSearchSONumber" class="form-control" placeholder="SONumber">
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="form-actions text-center">
                            <button type="button" id="btSearch" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                <i class="fa fa-search"></i> Search
                            </button>
                            <button type="button" id="btReset" class="btn blue-oleo">
                                <i class="fa fa-refresh"></i> Reset
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    @Html.Partial("_PanelSearchBegin")
    @Html.Partial("_PanelSearchZero")

    @*Search Result*@
    <div class="row panelSearchResult ">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption font-blue-steel">
                        <i class="fa fa-table"></i>
                        <span class="caption-subject">Search Result</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblSummaryData">
                        <thead>
                            <tr>
                                <th class="datatable-col-10 select-all-raw"><label id='cbALLData' class='mt-checkbox mt-checkbox-single mt-checkbox-outline'><input id="cbAll" type='checkbox' /><span></span></label></th>
                                <th class="datatable-col-100">SO Number</th>
                                <th class="datatable-col-100">Site ID</th>
                                <th class="datatable-col-200">Site Name</th>
                                <th class="datatable-col-100">Site ID Opr</th>
                                <th class="datatable-col-100">Status Master List</th>

                                <th class="datatable-col-50">Company MList</th>
                                <th class="datatable-col-50">Company Inv</th>
                                <th class="datatable-col-100">Customer</th>
                                <th class="datatable-col-100">RFI Date </th>
                                <th class="datatable-col-100">SLD Date </th>
                                <th class="datatable-col-100">BAPS Date </th>
                                <th class="datatable-col-100">Start Date BAPS </th>
                                <th class="datatable-col-100">End Date BAPS </th>
                                <th class="datatable-col-100">Tenant Type </th>
                                <th class="datatable-col-100">BaseLease Price </th>
                                <th class="datatable-col-100">Service Price </th>
                                <th class="datatable-col-100">Total Amount </th>
                                <th class="datatable-col-50">Currency </th>
                                <th class="datatable-col-100">Mio </th>
                                <th class="datatable-col-50">Month </th>
                                <th class="datatable-col-100">Start Date Accrue </th>
                                <th class="datatable-col-100">End Date Accrue </th>

                                <th class="datatable-col-50">D </th>
                                <th class="datatable-col-50">OD </th>
                                <th class="datatable-col-100">OD Category</th>
                                <th class="datatable-col-100">Region </th>

                                <th class="datatable-col-100">Type </th>
                                <th class="datatable-col-100">Department</th>
                                <th class="datatable-col-100">Status </th>
                                <th class="datatable-col-100">Remarks </th>
                                <th class="datatable-col-100">User Confirm File </th>
                                <th class="datatable-col-100">User Move File </th>

                            </tr>
                        </thead>
                    </table>

                    @*<input type="checkbox" id='inactivelist' value="inactivelist" />*@
                    <div class="row">
                        <button type="button" id="btConfirm" class="btn green">
                            <i class="fa fa-check"></i>Confirm
                        </button>
                        <button type="button" id="btMove" class="btn blue">
                            <i class="fa fa-arrow-right"></i> Move
                        </button>
                        <button type="button" id="btCancel" class="btn red">
                            <i class="fa fa-times"></i> Cancel
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row panelSiteData">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption font-blue-steel">
                        <i class="fa fa-table"></i>
                        <span class="caption-subject"><label id="lblDetailAdd">ListAdd</label></span>
                    </div>
                </div>
                <div class="portlet-body">

                    <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblAddData">
                        <thead>
                            <tr>
                                <th class="datatable-col-40">Action</th>
                                <th class="datatable-col-100">SO Number</th>
                                <th class="datatable-col-100">Site ID</th>
                                <th class="datatable-col-200">Site Name</th>
                                <th class="datatable-col-100">Site ID Opr</th>
                                <th class="datatable-col-100">Status Master List</th>

                                <th class="datatable-col-75">Company MList</th>
                                <th class="datatable-col-50">Company Inv</th>
                                <th class="datatable-col-100">Customer</th>
                                <th class="datatable-col-100">RFI Date </th>
                                <th class="datatable-col-100">SLD Date </th>
                                <th class="datatable-col-100">BAPS Date </th>
                                <th class="datatable-col-100">Start Date BAPS </th>
                                <th class="datatable-col-100">End Date BAPS </th>
                                <th class="datatable-col-100">Tenant Type </th>
                                <th class="datatable-col-100">BaseLease Price </th>
                                <th class="datatable-col-100">Service Price </th>
                                <th class="datatable-col-100">Total Amount </th>
                                <th class="datatable-col-50">Currency </th>
                                <th class="datatable-col-100">Mio </th>
                                <th class="datatable-col-50">Month </th>
                                <th class="datatable-col-100">Start Date Accrue </th>
                                <th class="datatable-col-100">End Date Accrue </th>

                                <th class="datatable-col-50">D </th>
                                <th class="datatable-col-50">OD </th>
                                <th class="datatable-col-100">OD Category</th>
                                <th class="datatable-col-100">Region </th>

                                <th class="datatable-col-100">Type </th>
                                <th class="datatable-col-100">Department</th>
                                <th class="datatable-col-100">Status </th>
                                <th class="datatable-col-100">Remarks </th>
                            </tr>
                        </thead>
                    </table>

                </div>
                <div class="portlet-body">
                    <div class="form-body" id="divMove" style="display:none">
                        <div class="row">
                            <div class="row col-md-6">
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">Department <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <select id="slDepartment" class="form-control select2"></select>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">File<i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                            <div class="form-control" data-trigger="fileinput"> <i class="glyphicon glyphicon-file"></i> <span class="fileinput-filename"></span></div>
                                            <span class="input-group-addon btn btn-default btn-file">
                                                <span class="fileinput-new">Select file</span><span class="fileinput-exists">Change</span>
                                                <input type="file" name="..." id="fuMove" required>
                                            </span>
                                            <a href="#" class="input-group-addon btn btn-red fileinput-exists" data-dismiss="fileinput">Remove</a>
                                        </div>
                                    </div>
                                </div>

                                <div class="row form-group">
                                    <label class="col-md-3 control-label">Remarks <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <textarea id="tbRemarksMove" rows="4" cols="50" class="form-control" maxlength="255"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-body" id="divConfirm" style="display:none">
                        <div class="row">
                            <div class="row col-md-6">
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">Target Date <i class="font-red">*</i></label>
                                    <div class="col-md-9">
                                        <div class="col-md-6">
                                            <input type="text" id="tbPeriode" size="16" class="form-control datepicker" placeholder="Input Month Period">
                                        </div>
                                        <div class="col-md-3">
                                            <select id="slPeriodeWeek" class="form-control select2" placeholder="Week"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">Rootcause <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <select id="slRootcause" class="form-control select2"></select>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">PICA <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <select id="slPica" class="form-control select2"></select>
                                    </div>
                                </div>
                                <div class="row form-group">
                                    <label class="col-md-3 control-label">PICA Detail <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <select id="slPicaDetail" class="form-control select2"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="row col-md-6">

                                <div class="row form-group">
                                    <label class="col-md-3 control-label">File<i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <div class="fileinput fileinput-new input-group" data-provides="fileinput">
                                            <div class="form-control" data-trigger="fileinput"> <i class="glyphicon glyphicon-file"></i> <span class="fileinput-filename"></span></div>
                                            <span class="input-group-addon btn btn-default btn-file">
                                                <span class="fileinput-new">Select file</span><span class="fileinput-exists">Change</span>
                                                <input type="file" name="..." id="fuConfirm" required>
                                            </span>
                                            <a href="#" class="input-group-addon btn btn-red fileinput-exists" id="btRemoveFile" data-dismiss="fileinput">Remove</a>
                                        </div>
                                    </div>
                                </div>

                                <div class="row form-group">
                                    <label class="col-md-3 control-label">Remarks <i class="font-red">*</i></label>
                                    <div class="col-md-7">
                                        <textarea id="tbRemarksConfirm" rows="4" cols="50" class="form-control" maxlength="255"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </div>
            <div class="row col-md-12 text-center">
                <button type="button" id="btSubmit" class="btn green">
                    <i class="fa fa-check"></i> Submit
                </button>

                <button type="button" id="btCancelAdd" class="btn red">
                    <i class="fa fa-times"></i> Cancel
                </button>
            </div>

        </div>
    </div>


</section>

<input type="hidden" id="hdAllowProcess" value="@ViewBag.AllowProcess" />
<input type="hidden" id="hdDocPath" value="@ViewBag.DocPath" />

<script src="@Url.Content("~/Content/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js")" type="text/javascript"></script>
<script src="@Url.Content("~/scripts/Accrue/UserConfirmation.js")" type="text/javascript"></script>
