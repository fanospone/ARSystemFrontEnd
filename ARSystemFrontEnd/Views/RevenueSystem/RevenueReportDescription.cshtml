@using ARSystemFrontEnd.Helper
@{
    ViewBag.Title = "Report Summary";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section id="pnlSummary">
    <div class="row filter">
        <div class="col-md-12">
            <div class="portlet box blue">
                <div class="portlet-title">
                    <div class="caption font-white">
                        <i class="fa fa-filter"></i>
                        <span class="caption-subject">Filter</span>
                    </div>
                    <div class="tools">
                        <a href="" class="collapse"> </a>
                    </div>
                </div>
                <div class="portlet-body form">
                    <form id="formSearch" class="form-horizontal" role="form">
                        <div class="form-body">
                            <div class="row">


                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Company</label>
                                        <div class="col-md-8">
                                            <select id="slsCompanyId" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Regional</label>
                                        <div class="col-md-8">
                                            <select id="slsRegionId" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Operator</label>
                                        <div class="col-md-8">
                                            <select id="slsCustomerId" class="form-control select2"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Department</label>
                                        <div class="col-md-8">
                                            <select id="slsDepartment" class="form-control select2"></select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Period RFI</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control datepicker" id="sStartRFIPeriod" readonly />
                                        </div>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control datepicker" id="sEndRFIPeriod" readonly />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">Period SLD</label>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control datepicker" id="sStartSLDPeriod" readonly />
                                        </div>
                                        <div class="col-md-3">
                                            <input type="text" class="form-control datepicker" id="sEndSLDPeriod" readonly />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="text-center">
                                        <button type="button" class="btn blue mt-ladda-btn ladda-button btSearch" data-style="zoom-in">
                                            <i class="fa fa-search"></i> Search
                                        </button>
                                        <button type="button" id="btReset" class="btn blue-oleo">
                                            <i class="fa fa-refresh"></i> Reset
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

    <div class="row panelSearchResult ">
        <div class="col-md-12">
            <div class="portlet light bordered">
                <div class="portlet-title">
                    <div class="caption font-blue-steel">
                        <i class="fa fa-table"></i>
                        <span class="caption-subject">Search Result</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered datatable-custom-width" id="tblReportRevenueDesc">
                        <thead>
                            <tr>
                                <th class="datatable-col-50">No</th> <!--1-->
                                <th class="datatable-col-100">SO Number</th> <!--2-->
                                <th  class="datatable-col-100">Site ID</th> <!--3-->
                                <th  class="datatable-col-150">Site Name</th> <!--4-->
                                <th class="datatable-col-50">STIP</th>  <!--5-->
                                <th class="datatable-col-150">Regional</th> <!--6-->
                                <th class="datatable-col-150">UserNumber</th> <!--7-->
                                <th class="datatable-col-150">Customer</th> <!--8-->
                                <th class="datatable-col-100">Site Status</th> <!--9-->
                                <th class="datatable-col-150">Contract Status</th> <!--10-->
                                <th class="datatable-col-150">Document Process</th> <!--11-->
                                <th class="datatable-col-150">Company</th> <!--12-->
                                <th class="datatable-col-150">RFI Date</th> <!--13-->
                                <th class="datatable-col-150">SLD Date</th> <!--14-->
                                <th class="datatable-col-150">BAPS Date</th> <!--15-->
                                <th class="datatable-col-100">Rental Start</th> <!--16-->
                                <th class="datatable-col-100">Rental End</th> <!--17-->
                                <th class="datatable-col-100">Tenant Type</th> <!--18-->
                                <th class="datatable-col-100 text-center">RF Inv</th> <!--19-->
                                <th class="datatable-col-100 text-center">MF Inv</th> <!--20-->
                                <th class="datatable-col-100 text-center">Price Base<br />on ML</th> <!--21-->
                                <th class="datatable-col-100 text-center">Price Base<br />on Invoice</th> <!--22-->
                                <th class="datatable-col-100 text-center">Last Inv Period</th> <!--23-->
                                <th class="datatable-col-100">Contract Start</th> <!--24-->
                                <th class="datatable-col-100">Contract End</th> <!--25-->
                                <th class="datatable-col-100 text-center">Balance Accrued</th> <!--26-->
                                <th class="datatable-col-100 text-center">Aging(days)</th> <!--27-->
                            </tr>
                            <tr>
                                <th>#</th> <!--1-->
                                <th><input type="text" class="form-control" id="sSoNumber"></th> <!--2-->
                                <th><input type="text" class="form-control" id="sSiteId"></th> <!--3-->
                                <th><input type="text" class="form-control" id="sSiteName"></th> <!--4-->
                                <th></th>  <!--5-->
                                <th></th> <!--6-->
                                <th></th> <!--7-->
                                <th></th> <!--8-->
                                <th></th> <!--9-->
                                <th></th> <!--10-->
                                <th></th> <!--11-->
                                <th></th> <!--12-->
                                <th></th> <!--13-->
                                <th></th> <!--14-->
                                <th></th> <!--15-->
                                <th></th> <!--16-->
                                <th></th> <!--17-->
                                <th></th> <!--18-->
                                <th></th> <!--19-->
                                <th></th> <!--20-->
                                <th></th> <!--21-->
                                <th></th> <!--22-->
                                <th></th> <!--23-->
                                <th></th> <!--24-->
                                <th></th> <!--25-->
                                <th></th> <!--26-->
                                <th></th> <!--27-->
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
<script src="@Url.ContentVersion("~/scripts/common.js")" type="text/javascript"></script>
<script src="@Url.ContentVersion("~/scripts/RevenueSystem/RevenueReportDescription.js")" type="text/javascript"></script>