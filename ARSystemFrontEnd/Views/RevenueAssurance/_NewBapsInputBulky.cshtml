
@{
    Layout = null;
}


<div class="portlet box blue">
    <div class="portlet-title">
        <div class="caption font-white">
            <i class="fa fa-calendar-o"></i>
            <span class="caption-subject">Project Information</span>
        </div>
        <div class="tools">
            <a href="" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body form">
        <form id="formSearchBapsUpload" class="form-horizontal" role="form">
            <div class="form-body">
                <div class="row">
                    <div class="col-md-8">
                        <div class="form-group">
                            <label class="col-md-3 control-label">BAPS Date</label>
                            <div class="col-md-8">
                                <input type="text" id="BapsDateBulky" class="form-control datepicker" required/>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-3 control-label">BAPS No</label>
                            <div class="col-md-8">
                                <input type="text" class="form-control" id="BapsNumberBulky" required/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-3 control-label">Document BAPS</label>
                            <div class="col-md-8">
                                <div class="fileinput fileinput-new" data-provides="fileinput">
                                    <div class="input-group">
                                        <div class="form-control uneditable-input input-fixed" data-trigger="fileinput">
                                            <i class="fa fa-file fileinput-exists"></i>&nbsp;
                                            <span class="fileinput-filename" id="file-upload-inputBulky"> </span><div></div>
                                        </div>
                                        <span class="input-group-addon btn default btn-file">
                                            <span class="fileinput-new"> Select file </span>
                                            <span class="fileinput-exists"> Change </span>
                                            <input type="file" name="postFileInputBulky" id="postFileInputBulky" accept="application/vnd.ms-excel, application/pdf" required>
                                        </span>
                                        <a href="javascript:;" class="input-group-addon btn red fileinput-exists" data-dismiss="fileinput"> Remove </a>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
<div class="form-actions">
    <button type="button" id="btAddSite" class="btn blue mt-ladda-btn ladda-button NeedRounding" data-style="zoom-in">
        <i class="fa fa-plus"></i> Add Site
    </button>
</div>
<div class="row panelTab margin-top-20">
    <div class="col-md-12">

        <div class="portlet-body">
            <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblSite">
                <thead>
                    <tr>
                        <th class="datatable-col-10 select-all-raw"></th>
                        <th class="datatable-col-70">SO Number</th>
                        <th class="datatable-col-120">Customer</th>
                        <th class="datatable-col-70">Site ID</th>
                        <th class="datatable-col-120">Site Name</th>
                        <th class="datatable-col-70">Site ID Customer</th>
                        <th class="datatable-col-120">Site Name Customer</th>

                        
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
<div class="text-center">
    <button type="submit" id="btnSubmitBaps" class="btn green mt-ladda-btn ladda-button" data-style="zoom-in">
        <i class="fa fa-check"></i> Submit
    </button>
    <button type="button" id="btnClearBaps" class="btn blue-oleo">
        <i class="fa fa-refresh"></i> Clear
    </button>
</div>
<div id="mdlAddSiteContainer"></div>

<script type="text/html" id="mdlAddSiteHtml">
    <div id="mdlAddSite" class="modal fade"  role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
        <div class="modal-dialog modal-xlg">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title">Add Site BAPS</h4>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="portlet box blue">
                                <div class="portlet-title">
                                    <div class="caption font-white">
                                        <i class="fa fa-filter"></i>
                                        <span class="caption-subject">Filter</span>
                                    </div>
                                    <div class="tools">
                                        <a href="" class="collapse"> </a>
                                    </div>
                                </div>
                                <div class="portlet-body form">
                                    <form id="formSearchSite" class="form-horizontal" role="form">
                                        <div class="form-body">
                                            <div class="form-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">Customer</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlCustomer" class="form-control select2"></select>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">Company</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlCopmany" class="form-control select2"></select>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">Product Type</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlProductType" class="form-control select2"></select>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">SO Number</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlSONumberMultiple" class="form-control select2" multiple></select>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">STIP Category</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlStipCategory" class="form-control select2"></select>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">BAUK Done</label>
                                                            <div class="col-md-9">
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        <div class="input-group">
                                                                            <input type="text" class="form-control" id="slmdlBaukDoneStart" aria-describedby="baukSdate-addon">
                                                                            <span class="input-group-addon" id="baukSdate-addon">
                                                                                <i class="fa fa-calendar" aria-hidden="true"></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-6">
                                                                        <div class="input-group">
                                                                            <input type="text" class="form-control" id="slmdlBaukDoneEnd" aria-describedby="baukEdate-addon">
                                                                            <span class="input-group-addon" id="baukEdate-addon">
                                                                                <i class="fa fa-calendar" aria-hidden="true"></i>
                                                                            </span>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-md-3 control-label">Siro</label>
                                                            <div class="col-md-9">
                                                                <select id="slmdlSiro" class="form-control select2"></select>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="text-center">
                                                <button type="submit" id="btnSearchBapsSite" class="btn blue mt-ladda-btn ladda-button" data-style="zoom-in">
                                                    <i class="fa fa-search"></i> Search
                                                </button>
                                                <button type="button" id="btResetBapsSite" class="btn blue-oleo">
                                                    <i class="fa fa-refresh"></i> Reset
                                                </button>
                                            </div>

                                        </div>
                                    </form>

                                </div>
                            </div>


                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <table class="table table-striped table-bordered table-hover datatable-custom-width" id="tblSearchBaps">
                                <thead>
                                    <tr>
                                        <th class="datatable-col-10 select-all-raw"></th>
                                        <th class="datatable-col-70">SO Number</th>
                                        <th class="datatable-col-125">Customer</th>
                                        <th class="datatable-col-70">Site ID</th>
                                        <th class="datatable-col-125">Site Name</th>
                                        <th class="datatable-col-125">Site ID Customer</th>
                                        <th class="datatable-col-125">Site Name Customer</th>
                                        <th class="datatable-col-100">Company ID</th>
                                        <th class="datatable-col-125">Company Name</th>
                                        <th class="datatable-col-50">Siro</th>
                                        <th class="datatable-col-125">STIP Number</th>
                                        <th class="datatable-col-125">STIP Category</th>
                                        <th class="datatable-col-125">Product Type</th>
                                        <th class="datatable-col-100">MLA Date</th>
                                        <th class="datatable-col-125">MLA Number</th>
                                        <th class="datatable-col-125">BAUK Number</th>
                                        <th class="datatable-col-100">BAUK Date</th>
                                        <th class="datatable-col-100">PO Number</th>
                                        <th class="datatable-col-100">PO Input Date</th>
                                        <th class="datatable-col-100">PO Date</th>
                                    </tr>
                                    <tr>
                                        <th class="datatable-col-10"></th>
                                        <th class="datatable-col-100">
                                            <div class="input-group input-group-sm">
                                                <input type="text" id="schMdlSONumber" class="form-control" />
                                                <span class="input-group-btn">
                                                    <button class="btn blue btn-small btnSearchGridBaps" type="button">
                                                        <i class="fa fa-search"></i>
                                                    </button>
                                                </span>
                                            </div>
                                        </th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-70"></th>
                                        <th class="datatable-col-125">
                                            <div class="input-group input-group-sm">
                                                <input type="text" id="schMdlSiteName" class="form-control" />
                                                <span class="input-group-btn">
                                                    <button class="btn blue btn-small btnSearchGridBaps" type="button">
                                                        <i class="fa fa-search"></i>
                                                    </button>
                                                </span>
                                            </div>
                                        </th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-100"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-50"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-100"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-125"></th>
                                        <th class="datatable-col-100"></th>
                                        <th class="datatable-col-100"></th>
                                        <th class="datatable-col-100"></th>
                                        <th class="datatable-col-100"></th>

                                    </tr>

                                </thead>
                            </table>

                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="text-center">
                                <button type="submit" id="btSaveAddSite" class="btn green mt-ladda-btn ladda-button" data-style="zoom-in">
                                    <i class="fa fa-check"></i> Add
                                </button>
                                <button type="button" id="btCloseAddSite" class="btn blue-oleo">
                                    <i class="fa fa-chevron-left"></i> Close
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
